<h1>New analytic</h1>

<%= error_messages_for :analytic %>

<% form_for(@analytic) do |f| %>
  <p>
    <b>Content</b><br />
    <%= f.text_field :content_id %>
  </p>

  <p>
    <b>Interaction</b><br />
    <%= f.text_field :interaction_id %>
  </p>

  <p>
    <b>User</b><br />
    <%= f.text_field :user_id %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', analytics_path %>
